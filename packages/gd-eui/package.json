{
    "name": "@editor/gd-eui",
    "version": "0.0.0",
    "maintainers": [
        {
            "name": "wenxing",
            "email": "wenxing@gaoding.com"
        },
        {
            "name": "jinjiu",
            "email": "jinjiu@gaoding.com"
        }
    ],
    "main": "lib/index.js",
    "module": "es/index.js",
    "typings": "src/index.ts",
    "scripts": {
        "build": "rollup -c ./scripts/rollup.config.js",
        "build:all": "sh ./build.sh",
        "demo": "cd ./service && pnpm i && pnpm run start && cd .. && cross-env --project=debugger vue-cli-service serve",
        "demo:service:dev": "cd ./service && pnpm i && pnpm run dev",
        "demo:service:start": "cd ./service && pnpm i && pnpm run start",
        "demo:client": "cross-env --project=debugger vue-cli-service serve",
        "docs:build": "pnpm i && NODE_OPTIONS=--max_old_space_size=4096 vuepress build docs",
        "docs:dev": "pnpm i && NODE_OPTIONS=--max_old_space_size=4096 vuepress dev docs --no-cache",
        "new": "node ./scripts/auto-create.js",
        "publish-gd": "pnpm version patch && node ./scripts/publish.js",
        "publish-beta": "node ./scripts/publish-beta.js",
        "publish-urgent": "pnpm version patch && node ./scripts/publish-urgent.js",
        "test": "vue-cli-service test:unit --coverage",
        "cypress:open": "cypress open-ct",
        "cypress:run": "cypress run-ct",
        "jscpd": "jscpd  --pattern 'src/**/*.ts'",
        "precss": "esno ./scripts/theme"
    },
    "dependencies": {
        "@gaoding/gd-antd": "^2.5.4",
        "lodash": "^4.17.15",
        "mitt": "3.0.0",
        "resize-observer-polyfill": "^1.5.1",
        "tinycolor2": "1.4.2"
    },
    "devDependencies": {
        "@babel/core": "7.15.8",
        "@cypress/vue": "^2.2.4",
        "@cypress/webpack-dev-server": "^1.8.1",
        "@rollup/plugin-babel": "5.3.0",
        "@rollup/plugin-commonjs": "21.0.0",
        "@rollup/plugin-json": "4.1.0",
        "@types/jest": "^24.0.19",
        "@types/lodash": "^4.14.182",
        "@types/tinycolor2": "^1.4.3",
        "@vue/cli-plugin-babel": "~5.0.4",
        "@vue/cli-plugin-typescript": "~5.0.4",
        "@vue/cli-plugin-unit-jest": "~5.0.4",
        "@vue/cli-service": "~5.0.4",
        "@vue/composition-api": "^1.4.2",
        "@vue/test-utils": "^1.0.3",
        "autoprefixer": "9.5.1",
        "axios": "^0.26.0",
        "core-js": "2",
        "cross-env": "^7.0.3",
        "cypress": "^9.5.0",
        "entities": "1.1.1",
        "esno": "^0.14.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "^4.5.2",
        "inquirer": "8.2.0",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "markdown-it": "8.4.2",
        "mockjs": "^1.1.0",
        "postcss": "^8.4.8",
        "postcss-cssnext": "2.11.0",
        "postcss-inline-svg": "4.1.0",
        "preprocess-to-css-variable": "^1.0.5",
        "prismjs": "1.26.0",
        "raw-loader": "^4.0.2",
        "rollup": "2.58.0",
        "rollup-plugin-less": "1.1.3",
        "rollup-plugin-node-resolve": "5.2.0",
        "rollup-plugin-postcss": "4.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "0.30.0",
        "rollup-plugin-vue": "5.0.0",
        "socket.io-client": "^4.4.1",
        "style-resources-loader": "^1.5.0",
        "typedoc": "0.22.11",
        "typescript": "4.4.3",
        "vue-json-editor": "^1.4.3",
        "vue-loader": "15.9.1",
        "vue-monaco-editor": "0.0.19",
        "vue-router": "3.2.0",
        "vue-template-compiler": "^2.6.11",
        "vuepress": "^1.8.2",
        "vuepress-plugin-typescript": "^0.3.1",
        "webpack": "^4.16.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.14",
        "xterm": "^4.17.0",
        "tslib": "^2.4.0"
    },
    "peerDependencies": {
        "@vue/composition-api": "^1.1.0",
        "vue": "^2.0.0 || >=3.0.0"
    }
}
