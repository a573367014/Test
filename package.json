{
  "name": "new-editor",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:growing-web": "turbo run dev --parallel --filter='@gaoding-es/*'",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@growing-web/web-builder": "^0.0.8",
    "chokidar-cli": "3.0.0",
    "turbo": "^1.2.16",
    "typescript": "^4.5.4"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
  }
}
